<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
                   namespace="Trakker.Data"
                   assembly="Trakker.Data">

<class name="Project" >
  <id name="Id">
    <generator class="identity" />
  </id>
  
  <property name="Name" />
  <property name="KeyName" />
  <property name="Lead" />
  <property name="Due" />
  <property name="Created" />
  <property name="TicketIndex" />
  <property name="Url" />
  <property name="ColorPaletteId" />

  <many-to-one name="ColorPalette" class="ColorPalette" column="ColorPaletteId" insert="false" update="false"/>
  <many-to-one name="Avatar" class="File" column="AvatarId" insert="false" update="false"/>

</class>

</hibernate-mapping>

