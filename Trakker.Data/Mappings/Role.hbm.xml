<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
                   namespace="Trakker.Data"
                   assembly="Trakker.Data">

<class name="Role">
  <id name="Id">
    <generator class="identity" />
  </id>
  
  <property name="Name" />
  <property name="Description" />


  <bag name="Users">
    <key column="RoleId" />
    <one-to-many class="User" />
  </bag>
</class>

</hibernate-mapping>

