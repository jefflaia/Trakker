<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
  xmlns="urn:nhibernate-mapping-2.2" namespace="Trakker.Data"
                   assembly="Trakker.Data">

<class name="ColorPalette">
  <id name="Id">
    <generator class="identity" />
  </id>
  
  <property name="NavBackgroundColor" />
  <property name="SubNavBackgroundColor" />
  <property name="HighlightColor" />
  <property name="NavTextColor" />
  <property name="SubNavTextColor" />
  <property name="LinkColor" />
  <property name="Name" />

</class>

</hibernate-mapping>

